<h1>Listing users</h1>

<%= link_to 'New user', new_user_path %>

<table>
  <tr>
    <th>Name</th>
    <th>E-mail</th>
  </tr>

  <% @user.each do |u| %>
    <tr>
      <td><%= u.name %></td>
      <td><%= u.email %></td>
      <td><%= link_to 'Show', user_path(u) %></td>
      <td><%= link_to 'Edit', edit_user_path(u) %></td>
    </tr>
  <% end %>
</table>

